DROP TABLE EMPLOYEE;

CREATE TABLE EMPLOYEE(
    EMPLOYEE_ID INT PRIMARY KEY AUTOINCREMENT,
    NAME VARCHAR(60), 
    STATUS BOOLEAN
);

INSERT INTO EMPLOYEE VALUES
