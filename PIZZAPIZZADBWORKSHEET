DROP TABLE CUSTOMER_ORDER;

create or replace table Customer_Order(
    customer_order_id int AUTOINCREMENT,
    phone_number VARCHAR(10),
    product_id int, 
    time_and_date TIMESTAMP_LTZ,
    primary key (customer_order_id, product_id)
);
 
