(2, 'Lean Obregon', TRUE),
(3, 'Marcus Villatoro', TRUE),
(4, 'Anuj Venkatesan',TRUE);

SELECT * FROM EMPLOYEE;

DROP TABLE CUSTOMER;

CREATE  TABLE CUSTOMER(
    PHONE_NUMBER VARCHAR(10) PRIMARY KEY,
    ZIP_CODE VARCHAR(6),
    CITY VARCHAR(64),
    STATE VARCHAR(2),
    ST_ADDRESS VARCHAR (64)
);

