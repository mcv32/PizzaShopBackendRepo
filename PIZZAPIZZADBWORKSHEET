CREATE TABLE Product(
	ID INT PRIMARY KEY autoincrement,
	Name VARCHAR(50),
	Size VARCHAR(50),
	Price FLOAT,
	Stock INT);
//FOREIGN KEY(ID) REFERENCES Customer_Order(Product_ID)


